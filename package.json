{
  "name": "quini6-resultados-api-crawler",
  "version": "1.0.2",
  "description": "Api en NodeJS/Express/Cheerio para obtener los resultados del Quini 6",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint --fix src test",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "db:tables": "npx sqlite3 .src/db/db '.tables'",
    "db:show": "npx sqlite3 ./src/db/db 'SELECT * FROM sorteos LIMIT 10;'",
    "start+db": "npm run dev & sleep 3 && npm run db:show"
  },
  "keywords": [],
  "author": "Enrique Becerra <kabeza@gmail.com> (https://beza.com.ar)",
  "repository": {
    "type": "git",
    "url": "https://github.com/kabeza/node_quini6.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "better-sqlite3": "^12.2.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "node-cron": "^4.2.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
  "@types/better-sqlite3": "^7.6.13",
  "@types/cors": "^2.8.17",
  "@types/express": "^5.0.1",
  "@types/jest": "^29.5.14",
  "@types/morgan": "^1.9.9",
  "@types/node": "^22.15.12",
  "@types/node-cron": "^3.0.11",
  "@types/supertest": "^6.0.3",
  "@types/cheerio": "^0.22.35",
  "@typescript-eslint/eslint-plugin": "^7.18.0",
  "@typescript-eslint/parser": "^7.18.0",
  "eslint": "^8.57.1",
  "eslint-config-airbnb-base": "^15.0.0",
  "eslint-config-airbnb-typescript": "^18.0.0",
  "eslint-import-resolver-typescript": "^4.3.4",
  "eslint-plugin-import": "^2.31.0",
  "jest": "^29.7.0",
  "nodemon": "^3.1.10",
  "supertest": "^7.1.0",
  "ts-jest": "^29.3.2",
  "ts-node": "^10.9.2",
  "typescript": "^5.8.3"
},
  "compilerOptions": {
    "strict": false,
    "noImplicitAny": false,
    "skipLibCheck": true
  },
"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}